{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-spaweb-api": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
                "base": "wwwroot",
                "browser": ""
            },
            "index": "src/index.html",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "assets": [
              "src/assets"
            ],
            "styles": [
                "src/styles.scss",
                "src/thirdparties/owl.carousel-modified.css",
                "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
                "node_modules/ngx-spinner/animations/ball-spin-clockwise.css",
                "node_modules/@swimlane/ngx-datatable/index.css",
                "node_modules/@swimlane/ngx-datatable/themes/material.css",
                "node_modules/@swimlane/ngx-datatable/themes/dark.css",
                "node_modules/@swimlane/ngx-datatable/themes/bootstrap.css"
            ],
            "scripts": [],
            "aot": false,
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "src/main.ts"
          },
          "configurations": {
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hmr.ts"
                }
              ]
            },
            "staging": {
                "fileReplacements": [
                    {
                        "replace": "src/environments/environment.ts",
                        "with": "src/environments/environment.staging.ts"
                    }
                ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
               "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "proxy.config.js",
            "buildTarget": "angular-spaweb-api:build"
          },
          "configurations": {
            "hmr": {
                "buildTarget": "angular-spaweb-api:build"
            },
            "staging": {
                "buildTarget": "angular-spaweb-api:build",
                "proxyConfig": "proxy.staging.js"
            },
            "production": {
                "buildTarget": "angular-spaweb-api:build",
                "proxyConfig": "proxy.production.js"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "angular-spaweb-api:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [ ],
            "styles": [
                "src/styles.scss",
                "src/thirdparties/owl.carousel-modified.css",
                "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
                "node_modules/ngx-spinner/animations/ball-spin-clockwise.css",
                "node_modules/@swimlane/ngx-datatable/index.css",
                "node_modules/@swimlane/ngx-datatable/themes/material.css",
                "node_modules/@swimlane/ngx-datatable/themes/dark.css",
                "node_modules/@swimlane/ngx-datatable/themes/bootstrap.css"
            ],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "angular-spaweb-api-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "angular-spaweb-api:serve"
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    },
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
